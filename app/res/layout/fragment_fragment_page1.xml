<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"

    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.guoyaohua.godseye.fragment.Fragment_page1">

    <com.baidu.mapapi.map.MapView
        android:id="@+id/tracing_mapView_f1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:clickable="true" />

    <Button
        android:id="@+id/bt_trace_f1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="25dp"
        android:layout_marginLeft="10dp"
        android:background="@color/jmui_default_cursor_color"
        android:elevation="50dp"
        android:text="轨迹查询" />

    <Button
        android:id="@+id/bt_loc_f1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/bt_trace_f1"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="22dp"
        android:layout_marginLeft="10dp"
        android:background="@color/jmui_default_cursor_color"
        android:elevation="50dp"
        android:text="实时位置" />
</RelativeLayout>
